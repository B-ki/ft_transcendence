{
  "name": "ft-transcendance-frontend",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path .gitignore",
    "lint:fix:report": "eslint ./src --output-file eslint_report.json --format json --ext .jsx,.js,.ts,.tsx --fix --ignore-path .gitignore",
    "format": "prettier --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json,html}\" ",
    "lint": "eslint ./src --ext .ts,.tsx --ignore-path .gitignore",
    "type-check": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "jwt-decode": "^3.1.2",
    "ky": "^0.33.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-notifications-component": "^4.0.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.9",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8.4.28",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "license": "MIT"
}
